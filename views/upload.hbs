<!DOCTYPE html>
<html>

<head>
	{{> header }}
	<title>Upload</title>
</head>
{{> navbar upload = 'true'}}

<body>
	<div class="content">
		<!-- Login Form -->
		<form action="/upload/upload" enctype="multipart/form-data" method="POST">
			<input type="file" name="uploadedFiles" value="uploading_file" multiple /><br><br>

			<input type="submit" class="btn btn-primary fadeIn fourth" value="Upload File"> </input><br><br>
			<small class="text-muted">{{message}}</small>
		</form>

	</div>
</body>

</html>